<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f5df">


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_LogIn_TenthScreen"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:textStyle="bold"
        android:textColor="#000"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/_30sdp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="60dp"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/_45sdp"
        android:layout_marginRight="@dimen/_30sdp"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_user"/>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="500dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/cards_shape"
        >

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/innerLeftGuideLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="@dimen/_30sdp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/innerRightGuideLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_end="@dimen/_30sdp" />


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/ChangePassword_Img"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:src="@drawable/ic_lock"
            app:layout_constraintLeft_toLeftOf="@id/innerLeftGuideLine"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/_15sdp"
            />
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/changePassword_Tv"
            android:layout_width="@dimen/_150sdp"
            android:layout_height="@dimen/_20sdp"
            android:text="Change Password"
            android:textColor="#000"
            android:textSize="@dimen/_15sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/_15sdp"
            app:layout_constraintHorizontal_bias="0.4"
            />


        <View
            android:id="@+id/view1"
            android:layout_width="@dimen/_180sdp"
            android:layout_height="2dp"
            android:background="#e9e9e9"
            app:layout_constraintTop_toBottomOf="@id/changePassword_Tv"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.5"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/notification_Img"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:src="@drawable/notification"
            app:layout_constraintLeft_toLeftOf="@id/innerLeftGuideLine"
            app:layout_constraintTop_toBottomOf="@id/view1"
            android:layout_marginTop="@dimen/_15sdp"
            />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/notification_Tv"
            android:layout_width="@dimen/_150sdp"
            android:layout_height="@dimen/_20sdp"
            android:text="Notification"
            android:textColor="#000"
            android:textSize="@dimen/_15sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view1"
            android:layout_marginTop="@dimen/_15sdp"
            app:layout_constraintHorizontal_bias="0.4"
            />


        <View
            android:id="@+id/view2"
            android:layout_width="@dimen/_180sdp"
            android:layout_height="2dp"
            android:background="#e9e9e9"
            app:layout_constraintTop_toBottomOf="@id/notification_Tv"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.5"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/contactUs_Img"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:src="@drawable/notification"
            app:layout_constraintLeft_toLeftOf="@id/innerLeftGuideLine"
            app:layout_constraintTop_toBottomOf="@id/view2"
            android:layout_marginTop="@dimen/_15sdp"
            />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/contactUs_Tv"
            android:layout_width="@dimen/_150sdp"
            android:layout_height="@dimen/_20sdp"
            android:text="Contact Us"
            android:textColor="#000"
            android:textSize="@dimen/_15sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view2"
            android:layout_marginTop="@dimen/_15sdp"
            app:layout_constraintHorizontal_bias="0.4"
            />

        <View
            android:id="@+id/view3"
            android:layout_width="@dimen/_180sdp"
            android:layout_height="2dp"
            android:background="#e9e9e9"
            app:layout_constraintTop_toBottomOf="@id/contactUs_Tv"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.5"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/termAndConditions_Img"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:src="@drawable/notification"
            app:layout_constraintLeft_toLeftOf="@id/innerLeftGuideLine"
            app:layout_constraintTop_toBottomOf="@id/view3"
            android:layout_marginTop="@dimen/_15sdp"
            />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/termAndConditions_Tv"
            android:layout_width="@dimen/_150sdp"
            android:layout_height="@dimen/_20sdp"
            android:text="Terms And Conditions"
            android:textColor="#000"
            android:textSize="@dimen/_15sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view3"
            android:layout_marginTop="@dimen/_15sdp"
            app:layout_constraintHorizontal_bias="0.4"
            />

        <View
            android:id="@+id/view4"
            android:layout_width="@dimen/_180sdp"
            android:layout_height="2dp"
            android:background="#e9e9e9"
            app:layout_constraintTop_toBottomOf="@id/termAndConditions_Tv"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.5"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/helpAndSupport_Img"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:src="@drawable/help"
            app:layout_constraintLeft_toLeftOf="@id/innerLeftGuideLine"
            app:layout_constraintTop_toBottomOf="@id/view4"
            android:layout_marginTop="@dimen/_15sdp"
            />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/helpAndSupport_Tv"
            android:layout_width="@dimen/_150sdp"
            android:layout_height="@dimen/_20sdp"
            android:text="Terms And Conditions"
            android:textColor="#000"
            android:textSize="@dimen/_15sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view4"
            android:layout_marginTop="@dimen/_15sdp"
            app:layout_constraintHorizontal_bias="0.4"
            />


        <com.google.android.material.button.MaterialButton
            android:layout_width="@dimen/_250sdp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@id/innerLeftGuideLine"
            app:layout_constraintTop_toBottomOf="@id/helpAndSupport_Tv"
            android:text="Loguot"
            app:cornerRadius="5dp"
            android:layout_marginTop="@dimen/_30sdp"/>














        <View
            android:id="@+id/viewThirdScreen"
            android:layout_width="150sp"
            android:layout_height="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="1dp"
            android:background="@drawable/view_design"/>




    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>