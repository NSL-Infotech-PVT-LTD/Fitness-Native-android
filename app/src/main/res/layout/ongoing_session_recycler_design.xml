<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:focusableInTouchMode="true"
    android:focusable="true">

    <com.google.android.material.card.MaterialCardView
        android:layout_margin="@dimen/_2sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:paddingBottom="@dimen/_20sdp"
        app:cardCornerRadius="@dimen/_10sdp"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/session_fragment_layoutdesign">
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_5sdp"
                android:background="@drawable/view_radius"
                app:layout_constraintTop_toTopOf="parent"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ongoingRecyclerRunImage"
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ic_run"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_20sdp"/>
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerSportsTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sports"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/poppins_regular"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerRunImage"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginLeft="@dimen/_5sdp"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerSportsName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/soccor"
                android:textSize="@dimen/_12sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:fontFamily="@font/poppins_regular"
                app:layout_constraintTop_toBottomOf="@id/ongoingRecyclerSportsTv"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerRunImage"
                android:textColor="@color/colorBlack"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ongoingRecyclerPersonImg"
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ic_person"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/ongoingRecyclerRunImage"
                android:layout_marginTop="@dimen/_25sdp"
                android:layout_marginLeft="@dimen/_20sdp"/>
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerOrganizationTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Organization"
                android:layout_marginLeft="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/poppins_regular"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerPersonImg"
                app:layout_constraintTop_toTopOf="@id/ongoingRecyclerPersonImg"/>
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerOrganizationNameTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_desc"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerPersonImg"
                app:layout_constraintTop_toBottomOf="@id/ongoingRecyclerOrganizationTv"
                android:layout_marginLeft="@dimen/_5sdp"
                android:fontFamily="@font/poppins_regular"
                android:textColor="@color/colorBlack"
                app:layout_constraintBottom_toBottomOf="parent"
                android:paddingBottom="@dimen/_10sdp"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ongoingRecyclerTimeImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_time"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerSportsTv"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginLeft="@dimen/_50sdp"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerTimeTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/time"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerTimeImg"
                android:layout_marginTop="@dimen/_20sdp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginLeft="@dimen/_5sdp"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/poppins_regular"/>
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/time_format"
                android:fontFamily="@font/poppins_regular"
                android:textSize="@dimen/_12sdp"
                app:layout_constraintTop_toBottomOf="@id/ongoingRecyclerTimeTv"
                app:layout_constraintLeft_toRightOf="@id/ongoingRecyclerTimeImg"
                android:layout_marginLeft="@dimen/_5sdp"
                android:textColor="@color/colorBlack"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ongoingRecyclerDateImg"
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ic_date"
                app:layout_constraintLeft_toLeftOf="@id/ongoingRecyclerTimeImg"
                app:layout_constraintTop_toBottomOf="@id/ongoingRecyclerRunImage"
                android:layout_marginTop="@dimen/_25sdp"/>
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/ongoingRecyclerDateTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/date"
                android:fontFamily="@font/poppins_regular"
                android:textSize="@dimen/_12sdp"
                app:layout_constraintLeft_toLeftOf="@id/ongoingRecyclerTimeTv"
                app:layout_constraintTop_toTopOf="@id/ongoingRecyclerDateImg" />

            <com.google.android.material.textview.MaterialTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/date_format"
                android:textSize="@dimen/_12sdp"
                app:fontFamily="@font/poppins_regular"
                app:layout_constraintTop_toBottomOf="@id/ongoingRecyclerDateTv"
                app:layout_constraintLeft_toLeftOf="@id/ongoingRecyclerDateTv"

                android:textColor="@color/colorBlack"
                android:fontFamily="@font/poppins_regular"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>





</androidx.constraintlayout.widget.ConstraintLayout>