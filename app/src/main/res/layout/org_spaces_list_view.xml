<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_10sdp"
    android:orientation="vertical">


    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="@dimen/_16sdp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/eventCardBgColor">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/discoverImageCard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="@dimen/_12sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                app:cardCornerRadius="@dimen/_20sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/findPlaceImage"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_100sdp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/ic_place_holder"
                    app:layout_constraintLeft_toLeftOf="parent" />


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/orgSpaceNameTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_20sdp"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/discoverRecylerName"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/_12sdp"
                app:layout_constraintLeft_toRightOf="@id/discoverImageCard"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/orgVenueText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/venue"
                android:textSize="@dimen/_8sdp"
                app:layout_constraintLeft_toLeftOf="@id/orgSpaceNameTv"
                app:layout_constraintTop_toBottomOf="@+id/orgSpaceNameTv" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/orgVenueNameTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="Sukhna Lake,California"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintLeft_toLeftOf="@+id/orgVenueText"
                app:layout_constraintTop_toBottomOf="@id/orgVenueText" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/orgPlaceTypeTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/place_type"
                android:textSize="@dimen/_8sdp"
                app:layout_constraintLeft_toLeftOf="@+id/orgVenueNameTv"
                app:layout_constraintTop_toBottomOf="@id/orgVenueNameTv" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/orgPlaceTypeNameTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="Park"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintLeft_toLeftOf="@+id/orgVenueText"
                app:layout_constraintTop_toBottomOf="@id/orgPlaceTypeTv" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/candidatesTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/candidate"
                android:textSize="@dimen/_8sdp"
                app:layout_constraintLeft_toLeftOf="@+id/orgVenueNameTv"
                app:layout_constraintTop_toBottomOf="@id/orgPlaceTypeNameTv" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/numberOfCandidate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text="40-50- Candidates"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintLeft_toLeftOf="@+id/orgVenueNameTv"
                app:layout_constraintTop_toBottomOf="@id/candidatesTv" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/findPlaceActualPriceTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_20sdp"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/price_display"
                android:textColor="@color/notificationRedIcon"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/numberOfCandidate" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>


</androidx.constraintlayout.widget.ConstraintLayout>