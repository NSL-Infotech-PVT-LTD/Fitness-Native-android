                app:layout_constraintTop_toTopOf="@+id/sessionCurrentTV"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/sessionCurrentTV"
                android:layout_marginRight="@dimen/_20sdp"></com.google.android.material.textview.MaterialTextView>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/sessionListRecycler"
                android:layout_width="match_parent"
