<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorLightGray"
            tools:context=".activities.OrganizationSignUpActivity">


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/completeProfileTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@null"
                android:fontFamily="@font/poppins_semibold"
                android:text="@string/complete_profile"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/_20sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/organization_coachTv"
                android:layout_width="@dimen/_70sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_20sdp"
                android:background="@drawable/round_background_colord"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center"
                android:padding="@dimen/_6sdp"
                android:text="@string/coach"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/_12sdp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/organization_Guideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.13" />

            <FrameLayout
                android:id="@+id/cardview1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:background="@drawable/cards_shape"
                app:layout_constraintTop_toTopOf="@+id/organization_Guideline">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayouttwo"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/organization_left_Guideline"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_begin="@dimen/_30sdp" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/organization_right_Guideline"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_end="@dimen/_30sdp" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_nameTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/name"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterNameEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/enter_name"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_nameTv" />



                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_emailTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/email"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toTopOf="@id/organization_enterNameEdt" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterEmailEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/enter_email"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_emailTv" />




                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_PhoneNoTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/phone"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toTopOf="@id/organization_enterEmailEdt" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterPhoneEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/enter_phone_number"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_PhoneNoTv" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_LocationTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/location"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toTopOf="@id/organization_enterPhoneEdt" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterLocationEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/enter_location"
                        android:drawableRight="@drawable/ic_my_location"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_LocationTv" />






                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_passTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/password"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toTopOf="@id/organization_enterLocationEdt" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterPasslEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/enter_password"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_passTv" />











                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_bioTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/bio"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_enterPasslEdt" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterBioEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/enter_bio"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_bioTv" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_Choose_ServicesTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/choose_services"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_enterBioEdt" />



                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/Select_Service_SpinnerTv"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_14sdp"
                        android:background="@drawable/card_input_edt_background"
                        android:padding="@dimen/_9sdp"
                        android:hint="Mohali sas Nagar"
                        android:paddingLeft="@dimen/_16sdp"
                        android:fontFamily="@font/poppins_semibold"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_Choose_ServicesTv" />

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/organization_SelectServiceSpinner"
                        style="@style/CustomSpinnerTheme"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:gravity="center_vertical"
                        android:padding="@dimen/_15sdp"
                        app:layout_constraintLeft_toLeftOf="@+id/Select_Service_SpinnerTv"
                        app:layout_constraintRight_toRightOf="@id/Select_Service_SpinnerTv"
                        app:layout_constraintTop_toTopOf="@+id/Select_Service_SpinnerTv">

                    </androidx.appcompat.widget.AppCompatSpinner>


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_BusinessHourstartTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/start_business_hour"
                        android:gravity="left"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_SelectServiceSpinner" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_BusinessHourendTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/end_business_hour"
                        android:gravity="left"
                        android:layout_marginRight="@dimen/_12sdp"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_SelectServiceSpinner" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_startTimeTv"
                        android:layout_width="@dimen/_115sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_22sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_regular"
                        android:gravity="center_vertical"
                        android:inputType="date"
                        android:hint="@string/time_format"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textSize="@dimen/_13sdp"
                        android:drawableLeft="@drawable/ic_time"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_BusinessHourstartTv"
                        />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_endTimeTv"
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_regular"
                        android:gravity="left|center"
                        android:inputType="date"
                        android:hint="@string/time_format"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textSize="@dimen/_13sdp"
                        android:drawableLeft="@drawable/ic_time"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_BusinessHourstartTv"
                        />









<!--                    <androidx.appcompat.widget.AppCompatSpinner-->
<!--                        android:id="@+id/organization_timeSpinner"-->
<!--                        android:layout_width="@dimen/_35sdp"-->
<!--                        android:layout_height="@dimen/_40sdp"-->
<!--                        android:layout_marginLeft="@dimen/_5sdp"-->
<!--                        android:layout_marginTop="@dimen/_5sdp"-->
<!--                        android:background="@drawable/organization_edt_design"-->
<!--                        app:layout_constraintLeft_toRightOf="@id/secondTv"-->
<!--                        app:layout_constraintTop_toBottomOf="@id/organization_BusinessHourTv" />-->



                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_ProfessionTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/profession"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_startTimeTv" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_professionTypeTv"
                        android:layout_width="@dimen/_115sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_regular"
                        android:hint="@string/type"
                        android:paddingLeft="@dimen/_16sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_ProfessionTv" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_ExpertiseTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_60sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/expertise"
                        app:layout_constraintRight_toLeftOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_endTimeTv" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_professionType2Tv"
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_regular"
                        android:hint="@string/type"
                        android:paddingLeft="@dimen/_16sdp"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_ProfessionTv" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_ExperienceDetailTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/experience_detail"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_professionTypeTv" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterExperienceDetailEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/experience"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_ExperienceDetailTv" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_TrainingServiceDetailTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/training_service_detail"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_enterExperienceDetailEdt" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/organization_enterTrainingServiceDetailEdt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@drawable/organization_edt_design"
                        android:fontFamily="@font/poppins_semibold"
                        android:gravity="center_vertical"
                        android:hint="@string/service_details"
                        android:paddingLeft="@dimen/_16sdp"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_TrainingServiceDetailTv" />


                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_HourlyRateTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_25sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:text="@string/hourly_rate"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintLeft_toRightOf="@id/organization_left_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_enterTrainingServiceDetailEdt" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/organization_DollarSignTv"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:fontFamily="@font/poppins_regular"
                        android:gravity="center"
                        android:text="@string/dollar_sign_value"
                        android:textSize="@dimen/_12sdp"
                        app:layout_constraintRight_toLeftOf="@id/organization_EnterHourlyRateEdt"
