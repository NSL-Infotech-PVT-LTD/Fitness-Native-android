                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_enterTrainingServiceDetailEdt" />

                    <com.google.android.material.button.MaterialButton
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:layout_marginBottom="@dimen/_100sdp"
                        android:backgroundTint="@color/colorWhite"
                        android:fontFamily="@font/poppins_regular"
                        android:gravity="center"
                        android:textSize="@dimen/_12sdp"
                        android:hint="Edit"
                        android:textAllCaps="false"
                        android:textColor="@color/colorBlack"
                        app:cornerRadius="@dimen/_5sdp"
                        app:layout_constraintLeft_toLeftOf="@id/organization_left_Guideline"
                        app:layout_constraintRight_toRightOf="@id/organization_right_Guideline"
                        app:layout_constraintTop_toBottomOf="@id/organization_EnterHourlyRateEdt"
                        app:strokeColor="@color/colorGreen"
                        app:strokeWidth="@dimen/_1sdp" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </FrameLayout>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/profileImageView"
                android:layout_width="@dimen/_100sdp"
                android:layout_height="@dimen/_100sdp"
                android:layout_marginTop="@dimen/_20sdp"
                app:cardCornerRadius="@dimen/_8sdp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toTopOf="@+id/cardview1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/cardview1">

                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/sophie" />
            </com.google.android.material.card.MaterialCardView>

